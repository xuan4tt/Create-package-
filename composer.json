        "xuan4tt",
        "create-package"
    ],
    "homepage": "https://github.com/xuan4tt/create-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nguyễn Trường Xuân",
            "email": "xuanntph07568@fpt.edu.vn",
            "role": "Developer"
        }
